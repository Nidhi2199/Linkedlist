#include<bits/stdc++.h>

using namespace std;
class node{
    public :
    int data;
    node *next;
};
    int main(){
        int n; // #elements
        cin>>n;
        node* head,*p,*q;
        head=NULL;
        for(int i=0;i<n;i++)
        { 
          p = new node(); int x;
            cin>>x;
            p->data=x;
            p->next=NULL;
            if(head==NULL){
                head=p;
                q=head;
            }
            else{
                q->next=p;
                q=p;
                
            }}
            node*s=head;
             while(s!=NULL){
                cout<<s->data;
                 s=s->next;
             }
            
        }
